    "coc.preferences.formatOnSaveFiletypes": [
        "jsonc",
        "json",
        "css",
        "scss",
        "typescript",
        "javascript",
        "typescriptreact",
        "javascriptreact"
    ],
    "scss.lint.unknownProperties": "ignore",
    "css.lint.unknownProperties": "ignore",
    "yaml.schemas": {
        "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.7.json": "/docker-compose.*yml"
    },
    "languageserver": {
        "docker": {
            "enable": false,
            "command": "docker-langserver",
            "args": ["--stdio"],
            "filetypes": ["Dockerfile"]
        },
        "haskell": {
            "enable": false,

            "command": "stack",
            "args": ["exec", "ghcide", "--", "--lsp"],
            "rootPatterns": [
                ".stack.yaml",
                ".hie-bios",
                "BUILD.bazel",
                "cabal.config",
                "package.yaml"
            ],
            "filetypes": ["hs", "lhs", "haskell"]
